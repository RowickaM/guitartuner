<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="CustomSpinner" parent="Widget.AppCompat.TextView.SpinnerItem">
        <item name="android:textColor">@color/text</item>
    </style>
    <style name="seekBar" parent="Widget.AppCompat.SeekBar">
        <item name="android:progressBackgroundTint">@color/text</item>
        <item name="android:progressTint">@color/green</item>
        <item name="android:colorControlActivated">@color/green</item>
    </style>

</resources>
